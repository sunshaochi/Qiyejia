<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_color"
    android:orientation="vertical">
    <include layout="@layout/title_act_layout"></include>

    <LinearLayout
        android:id="@+id/llContract"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <View style="@style/view_line"></View>

        <com.boyuanitsm.tools.view.CommonView
            app:notesTextSize="14sp"
            app:notesTextColor="#666666"
            app:desTextColor="#333333"
            app:desTextSize="14sp"
            app:isShowArrowImage="false"
            app:isShowCategoryImage="false"
            android:id="@+id/cv_name"
            style="@style/cv_style"
            app:desText="企业名称"
            app:statusImage="@mipmap/email"></com.boyuanitsm.tools.view.CommonView>

        <View style="@style/view_line"></View>

        <com.boyuanitsm.tools.view.CommonView
            app:notesTextSize="14sp"
            app:notesTextColor="#666666"
            app:desTextColor="#333333"
            app:desTextSize="14sp"
            app:isShowArrowImage="false"
            app:isShowCategoryImage="false"
            android:id="@+id/cv_phone"
            style="@style/cv_style"
            app:desText="联系电话"
            app:statusImage="@mipmap/internet"></com.boyuanitsm.tools.view.CommonView>

        <View style="@style/view_line"></View>

        <com.boyuanitsm.tools.view.CommonView
            app:notesTextSize="14sp"
            app:notesTextColor="#666666"
            app:desTextColor="#333333"
            app:desTextSize="14sp"
            app:isShowArrowImage="false"
            app:isShowCategoryImage="false"
            android:id="@+id/cv_hangye"
            style="@style/cv_style"
            app:desText="所属行业"
            app:statusImage="@mipmap/positon"

            ></com.boyuanitsm.tools.view.CommonView>
        <View style="@style/view_line"></View>

        <com.boyuanitsm.tools.view.CommonView
            app:notesTextSize="14sp"
            app:notesTextColor="#666666"
            app:desTextColor="#333333"
            app:desTextSize="14sp"
            app:isShowArrowImage="false"
            app:isShowCategoryImage="false"
            android:id="@+id/cv_guangwang"
            style="@style/cv_style"
            app:desText="官网地址"
            app:statusImage="@mipmap/positon"

            ></com.boyuanitsm.tools.view.CommonView>
        <View style="@style/view_line"></View>

        <com.boyuanitsm.tools.view.CommonView
            app:notesTextSize="14sp"
            app:notesTextColor="#666666"
            app:desTextColor="#333333"
            app:desTextSize="14sp"
            app:isShowArrowImage="false"
            app:isShowCategoryImage="false"
            android:id="@+id/cv_adress"
            style="@style/cv_style"
            app:desText="公司地址"
            app:statusImage="@mipmap/positon"
            ></com.boyuanitsm.tools.view.CommonView>
        <View style="@style/view_line"></View>
    </LinearLayout>


</LinearLayout>