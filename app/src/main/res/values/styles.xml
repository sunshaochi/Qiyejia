<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="view_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/line_color</item>

    </style>

    <!--标题样式-->
    <style name="tv_title_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="tv_12_white">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="tv_14_white">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="tv_12_grey">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/tv_grey</item>
    </style>

    <style name="tv_14_black">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#333333</item>
    </style>
    <!--首页-->
    <style name="linear_main">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="tv_main_btm">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">@color/tv_blue_color</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>

    <style name="mine_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="com.boyuanitsm.echinfo:leftTextSize">16sp</item>
    </style>

    <style name="mineAct_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="com.boyuanitsm.echinfo:rightTextSize">14sp</item>
        <item name="com.boyuanitsm.echinfo:leftTextSize">16sp</item>
        <item name="com.boyuanitsm.echinfo:leftTextColor">@color/black</item>
        <item name="com.boyuanitsm.echinfo:isShowLeftImg">false</item>
    </style>

    <style name="tv_shareDialog">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">8dp</item>
        <item name="android:background">@drawable/item_back</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="ppAnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/popu_in</item>
        <item name="android:windowExitAnimation">@anim/popu_out</item>
    </style>

    <style name="btn_blue">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:background">@drawable/blue_btn</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_margin">15dp</item>
    </style>

    <style name="executed_item_tv_black">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="executed_item_tv_gray" parent="executed_item_tv_black">
        <item name="android:textColor">#999999</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>

    <style name="productInfo_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@color/white</item>
        <item name="com.boyuanitsm.echinfo:leftTextSize">14sp</item>
        <item name="com.boyuanitsm.echinfo:leftTextColor">#333333</item>
        <item name="com.boyuanitsm.echinfo:isShowLeftImg">false</item>
        <item name="com.boyuanitsm.echinfo:rightTextSize">14sp</item>
        <item name="com.boyuanitsm.echinfo:rightTextColor">#999999</item>
        <item name="com.boyuanitsm.echinfo:isShowArrowRight">false</item>
    </style>

    <style name="tv_14_black_bold" parent="tv_14_black">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="tv_16_black_bold" parent="tv_14_black">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="tv_14_white_bold" parent="tv_14_white">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="tv_14_gray" parent="tv_14_black">
        <item name="android:textColor">#999999</item>
    </style>

    <style name="tv_12_blue" parent="tv_12_white">
        <item name="android:textColor">@color/tv_blue_color</item>
    </style>

    <style name="tv_creditInfo_grayItem" parent="tv_14_gray">
        <item name="android:drawablePadding">8dp</item>
        <item name="android:drawableLeft">@mipmap/ic_icon</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="ll_lg_bg">
        <item name="android:padding">15dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="ll_creDetail">
        <item name="android:paddingLeft">15dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>

    <style name="tv_creDetail_black_bold" parent="tv_16_black_bold">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">44dp</item>
    </style>

    <style name="tv_creDetail_black" parent="tv_14_black">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:paddingTop">15dp</item>
    </style>

    <style name="miv_patentDetail_item" parent="productInfo_item">
        <item name="android:layout_height">45dp</item>
    </style>

    <style name="ll_businessChild_one">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingTop">10dp</item>
    </style>

    <style name="tvBlack_businessChild_one" parent="tv_14_black_bold">
        <item name="android:layout_height">33dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">15dp</item>
    </style>
    <style name="tvGray_businessChild_one" parent="tv_14_gray">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="view_line_vertical" parent="view_line">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">1px</item>
    </style>

    <style name="cv_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:background">@drawable/item_back</item>
        <item name="com.boyuanitsm.echinfo:statusImage_height">15dp</item>
        <item name="com.boyuanitsm.echinfo:statusImage_width">15dp</item>
        <item name="com.boyuanitsm.echinfo:desTextSize">14sp</item>
        <item name="com.boyuanitsm.echinfo:desTextColor">#3a3a3a</item>

    </style>

    <style name="ll_secter_title">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="text_secter_title">
        <item name="android:gravity">center</item>
        <item name="android:textColor">#333333</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="iv_secter_title">
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:src">@mipmap/xj</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

</resources>
